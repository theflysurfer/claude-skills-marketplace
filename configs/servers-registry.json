{
  "version": "1.0.0",
  "description": "Registre des serveurs et applications au démarrage Windows",
  "startup_folder": "%APPDATA%\\Microsoft\\Windows\\Start Menu\\Programs\\Startup",
  "servers": {
    "idle-queue": {
      "name": "Idle Queue Service",
      "description": "Exécute les tâches en arrière-plan quand le PC est idle",
      "port": 8742,
      "start_command": "pythonw -m idle_queue.cli start",
      "stop_command": "pythonw -m idle_queue.cli stop",
      "health_check": "http://localhost:8742/health",
      "startup_file": "idle-queue.bat",
      "startup": true,
      "category": "core"
    },
    "mkdocs": {
      "name": "MkDocs Documentation",
      "description": "Serveur de documentation locale du marketplace",
      "port": 8000,
      "start_command": "mkdocs serve -a localhost:8000",
      "cwd": "$MARKETPLACE_ROOT",
      "startup_file": "Claude Marketplace Docs.lnk",
      "startup": true,
      "category": "docs"
    },
    "cooking-manager": {
      "name": "Cooking Manager Server",
      "description": "Serveur pour l'app de gestion des courses Auchan",
      "startup_file": "Cooking Manager Server.lnk",
      "startup": true,
      "category": "app"
    },
    "jokers": {
      "name": "Jokers Hockey",
      "description": "Site web Jokers Hockey (Express/TypeScript)",
      "port": 5000,
      "start_command": "npm run dev",
      "cwd": "C:\\Users\\julien\\OneDrive\\Coding\\_Projets de code\\2025.11 Site Web Jokers",
      "startup": false,
      "category": "project"
    },
    "money-manager": {
      "name": "Money Manager",
      "description": "App gestion finances personnelles (Next.js)",
      "port": 3000,
      "start_command": "npm run dev",
      "cwd": "C:\\Users\\julien\\OneDrive\\Coding\\_Projets de code\\2025.12 Money Manager\\money-manager",
      "startup": true,
      "startup_file": "Money Manager.lnk",
      "category": "project"
    }
  },
  "utilities": {
    "ahk-command-picker": {
      "name": "AHK Command Picker",
      "description": "Launcher de commandes AutoHotkey",
      "startup_file": "AHK Command Picker.lnk"
    },
    "ahk-scripts": {
      "name": "Script Perso AHK",
      "description": "Scripts AutoHotkey personnels",
      "startup_file": "Script Perso AHK.lnk"
    },
    "listary": {
      "name": "Listary",
      "description": "Recherche rapide de fichiers",
      "startup_file": "Listary.lnk"
    },
    "quicklook": {
      "name": "QuickLook",
      "description": "Preview fichiers avec espace",
      "startup_file": "QuickLook.lnk"
    },
    "sharex": {
      "name": "ShareX",
      "description": "Captures d'écran et enregistrement",
      "startup_file": "ShareX.lnk"
    },
    "windowtop": {
      "name": "WindowTop",
      "description": "Fenêtres toujours au premier plan",
      "startup_file": "WindowTop.lnk"
    }
  },
  "categories": {
    "core": {
      "name": "Core Services",
      "description": "Services essentiels du système Claude"
    },
    "docs": {
      "name": "Documentation",
      "description": "Serveurs de documentation"
    },
    "app": {
      "name": "Applications",
      "description": "Applications personnelles"
    },
    "project": {
      "name": "Projets Dev",
      "description": "Serveurs de développement de projets"
    }
  }
}
